    "end": {
      "dateTime": "2025-01-29T10:30:00-08:00",
      "timeZone": "America/Los_Angeles"
    },

    "recurringEventId": "recurring_abc123",
    "originalStartTime": {
      "dateTime": "2025-01-29T10:00:00-08:00",
      "timeZone": "America/Los_Angeles"
    },

    "iCalUID": "recurring_abc123@google.com",

    "_observations": {
      "different_id": "Instance ID includes timestamp suffix",
      "recurringEventId": "Points back to parent event",
      "originalStartTime": "Identifies which occurrence this is",
      "same_iCalUID": "All instances share same iCalUID"
    }
  },

  "instance_2_modified": {
    "_comment": "Second instance (Feb 5) - moved to 11 AM",
    "kind": "calendar#event",
    "id": "recurring_abc123_20250205T180000Z",
