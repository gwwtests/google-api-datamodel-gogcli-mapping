    "recurrence": [
      "RRULE:FREQ=WEEKLY;BYDAY=WE;COUNT=10"
    ],

    "iCalUID": "recurring_abc123@google.com",

    "_observations": {
      "recurrence_field": "RRULE in RFC 5545 format",
      "timeZone_required": "Required for recurring events (DST handling)",
      "this_is_parent": "This is the recurring event definition, not an instance"
    }
  },

  "instance_1_unchanged": {
    "_comment": "First instance (Jan 29) - not modified",
    "kind": "calendar#event",
    "id": "recurring_abc123_20250129T180000Z",
